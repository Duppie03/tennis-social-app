{
  "name": "mzansi-court-q",
  "version": "1.0.0",
  "description": "Eldoraigne Tennis Club Social Day Manager",
  "_main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "build:parcel": "parcel build public/index.html --dist-dir dist --public-url . --no-scope-hoist --no-optimize",
    "build:copy": "xcopy /E /I /Y public\\source dist\\source",
    "build": "npm run build:parcel && npm run build:copy",
    "package": "npm run build && pkg ."
  },
  "keywords": [
    "tennis",
    "club",
    "manager",
    "mzansi",
    "south africa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "nodemon": "^3.0.1",
    "parcel": "^2.10.3",
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pkg": {
    "assets": [
      "dist/**/*"
    ],
    "targets": [
      "node16-win-x64"
    ]
  }
}